#version 450
layout(local_size_x = 256, local_size_y = 1, local_size_z = 1) in;

layout(binding = 0, std430) readonly buffer ConfigBuf
{
    uint size;
    uint value;
} _19;

layout(binding = 1, std430) buffer TargetBuf
{
    uint data[];
} _32;

void main()
{
    uint ix = gl_GlobalInvocationID.x;
    if (ix < _19.size)
    {
        _32.data[ix] = _19.value;
    }
}

